    "@tsed/testing": "4.7.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "concurrently": "^3.5.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "method-override": "^2.3.10",
    "mongoose": "^5.0.9",
    "passport": "^0.4.0",
